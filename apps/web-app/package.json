        "@types/react-dom": "18.3.1",
        "@workleap/browserslist-config": "2.0.1",
        "@workleap/eslint-plugin": "3.2.3",
        "@workleap/typescript-configs": "3.0.2",
        "browserslist": "4.24.2",
        "eslint": "8.57.0",
        "http-server": "14.1.1",
        "nodemon": "3.1.7",
        "storybook": "8.4.5",
        "typescript": "5.5.4"
    },
    "dependencies": {
        "@rslib-poc/components-lib": "workspace:*",
        "@rslib-poc/ext-lib": "workspace:*",
